<DiffuseAstrocyticAndOligodendroglialTumours rdf:about="#AnaplasticOligodendroglioma,IDH-mutantAnd1p/19q-codeleted">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
  <nameAlso xml:lang="cn">恶性少突胶质细胞瘤,IDH突变型和1p/19q联合缺失</nameAlso>
