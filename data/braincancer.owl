  <NameInCN rdf:datatype="http://www.w3.org/2001/XMLSchema#string">朗格汉斯细胞组织细胞增生症</NameInCN>
  <ICD_10Code rdf:datatype="http://www.w3.org/2001/XMLSchema#string">C96.5</ICD_10Code>
  <ICDThematicWord rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Other and unspecified malignant neoplasms of lymphoid, haematopoietic and related tissue.Multifocal and unisystemic Langerhans-cell histiocytosis</ICDThematicWord>
  <ICD_OCode rdf:datatype="http://www.w3.org/2001/XMLSchema#string">9751/3</ICD_OCode>
